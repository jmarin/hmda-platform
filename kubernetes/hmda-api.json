{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "labels": {
      "app": "hmda"
    },
    "name": "hmda-api"
  },
  "spec": {
    "type": "NodePort",
    "ports": [
      {
        "name": "filing",
        "port": 8080,
        "protocol": "TCP",
        "targetPort": 8080
      },
      {
        "name": "admin",
        "port": 8081,
        "protocol": "TCP",
        "targetPort": 8081
      },
      {
        "name": "public",
        "port": 8082,
        "protocol": "TCP",
        "targetPort": 8082
      },
      {
        "name": "cluster",
        "port": 19999,
        "protocol": "TCP",
        "targetPort": 19999
      }
    ],
    "selector": {
      "app": "hmda"
    }
  }
}
